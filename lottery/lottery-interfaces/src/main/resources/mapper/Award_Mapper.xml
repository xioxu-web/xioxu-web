<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.lottery.infrastructure.dao.IAwardDao">
    <select id="queryAwardInfo" parameterType="java.lang.String" resultType="com.example.lottery.infrastructure.po">
      select id,awardId,awardType,awardType,awardName,awardContent,createTime,updateTime
      from award
      where awardId=#{awardId}
    </select>
</mapper>
