<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.spring.middleware.db.router.mapper.UserMapper">
    <!--字段实体映射-->
    <resultMap id="BaseResultMap" type="com.spring.middleware.db.router.domain.UserInfo">
        <result property="userId" column="user_id"/>
        <result property="pswd" column="pswd"/>
        <result property="username" column="usrename"/>
    </resultMap>
    <select id="findAll"  resultMap="BaseResultMap">
        SELECT*FROM userinfo;
    </select>
</mapper>
